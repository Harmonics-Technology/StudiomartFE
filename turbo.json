{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "outputs": []
    }
  },
  "globalEnv": [
    "NEXT_PUBLIC_CLIENT_URL",
    "NEXT_PUBLIC_API_BASEURL",
    "NEXT_PUBLIC_VENDOR_URL",
    "NEXT_PUBLIC_API_GOOGLE_API_KEY"
  ],
  "globalDependencies": [
    ".env"
  ]
}
{
    "name": "studiomart-client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon --typescript public/icons",
        "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
        "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
        "test": "jest -c test/jest.config.js",
        "schema:fetch": "node ./script/fetch-schema.js",
        "schema:hooks": "openapi --input ./src/types/api-schema.json --output ./src/types",
        "schema:generate": "node ./script/schema-codegen.js --srcPath ./src/types/api-schema.json --destPath ./src/types/api --separatedFiles true",
        "schema:codegen": "yarn schema:fetch && yarn schema:generate",
        "format-staged": "pretty-quick --staged",
        "openapi:codegen": "openapi --input https://studiomartbe.azurewebsites.net/swagger/v1/swagger.json --output ./src/services --client axios --useOptions"
    },
    "dependencies": {
        "@chakra-ui/icons": "^2.0.15",
        "@chakra-ui/react": "^2.4.7",
        "@codegena/oapi3ts-cli": "^2.1.10-alpha.8",
        "@emotion/react": "^11.1.4",
        "@emotion/styled": "^11.0.0",
        "@hookform/resolvers": "^2.9.7",
        "@types/js-cookie": "^3.0.2",
        "axios": "^0.21.1",
        "framer-motion": "^8.4.2",
        "js-cookie": "^3.0.1",
        "mobx": "^6.0.4",
        "mobx-react": "^7.0.5",
        "mobx-state-tree": "^5.0.1",
        "next": "^12.1.6",
        "next-compose-plugins": "^2.2.1",
        "nextjs-progressbar": "^0.0.16",
        "openapi-typescript-codegen": "^0.23.0",
        "prettier": "^2.2.1",
        "react": "^18.2.0",
        "react-datepicker": "^4.8.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.39.5",
        "react-hot-toast": "^2.4.0",
        "react-icons": "^4.9.0",
        "react-multi-date-picker": "^3.3.4",
        "react-responsive-carousel": "^3.2.23",
        "react-selectrix": "^1.0.17",
        "react-spinners": "^0.13.7",
        "react-text-transition": "^3.0.2",
        "styled-components": "^5.2.1",
        "styled-system": "^5.1.5",
        "ui": "*",
        "use-debounce": "^9.0.2",
        "yup": "^0.32.11",
        "yup-password": "^0.2.2"
    },
    "devDependencies": {
        "@svgr/cli": "^5.5.0",
        "@testing-library/react": "^11.2.3",
        "@testing-library/react-hooks": "^5.0.0",
        "@types/node": "^14.14.13",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "dotenv": "^8.2.0",
        "eslint-config-custom": "*",
        "husky": "^4.3.7",
        "identity-obj-proxy": "^3.0.0",
        "isomorphic-unfetch": "^3.1.0",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.3",
        "next-transpile-modules": "^10.0.0",
        "nock": "^13.0.6",
        "ts-jest": "^26.4.4",
        "tsconfig": "*",
        "typescript": "^4.1.3"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "npm run lint -- --quiet --fix"
        ]
    },
    "resolutions": {
        "@types/react": "17.0.38"
    }
}

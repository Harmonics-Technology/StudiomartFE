{
  "compilerOptions": {
    "target": "es5",
    "downlevelIteration": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "react": ["./node_modules/@types/react"],
      "@definitions/*": ["src/definitions/*"],
      "@definitions": ["src/definitions"],
      "@components/*": ["src/components/*"],
      "@components": ["src/components"],
      "@public/*": ["public/*"],
      "@public": ["public"],
      "@styles/*": ["src/styles/*"],
      "@styles": ["src/styles"],
      "@components/icons/*": ["src/components/icons/*"],
      "@components/icons": ["src/components/icons"],
      "@mobx/*": ["src/mobx/*"],
      "@mobx": ["src/mobx"],
      "@test/*": ["test/*"],
      "@test": ["test"]
    },
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/homeandcategory/Component/Navbar.js"
, "../../packages/shared/ui/Components/utils/ResponseBox.tsx", "../../packages/shared/ui/Components/utils/BookingsBtn.tsx", "../../packages/shared/ui/Components/utils/StudioCard.tsx", "../../packages/shared/ui/Components/utils/GlobalSearch.tsx", "../../packages/shared/ui/Components/utils/SearchBox.tsx"  ],
  "exclude": ["node_modules", "test/*"]
}
